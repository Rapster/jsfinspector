<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0"
                id="inspect">
    
    <description>
        VDL Documentation for components of JSFInspector.
    </description>
    
    <namespace>http://thomas-asel.de/jsfinspector</namespace>
        
    <tag>
        <tag-name>tree</tag-name>
        <component>
            <component-type>InspectTree</component-type>
            <renderer-type>ClientStubRenderer</renderer-type>
        </component>
        <description>
            Renders a list containing the names and number of occurence for every component (composite an non-composite) in the view this component was placed in.
        </description>
    </tag>
    
    <tag>
        <tag-name>lifecycle</tag-name>
        <component>
            <component-type>InspectLifecycle</component-type>
            <renderer-type>ClientStubRenderer</renderer-type>
            <description>Renders a visual representation of the lifecycle phases.</description>
        </component>
    </tag>
        
</facelet-taglib>